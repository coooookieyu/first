#include <bits/stdc++.h>
using namespace std;

int main(){
	double money;
	cin>>money;
	double b[10]={100,50,20,10,5,2,1,0.5,0.2,0.1};
	int cot[10]={0};
	int change=money*10;
	for(int i=0;i<10;i++){
		cot[i]=change/((int)(b[i]*10));
		change%=((int)(b[i]*10));
	}
	for(int i=9;i>=0;i--){
		cout<<b[i]<<" "<<cot[i]<<endl;
	}
	return 0;
} 
